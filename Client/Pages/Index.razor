@page "/"

<PageTitle>Blazor Full Stack Crud (SQLITE)</PageTitle>

<div class="container mt-3">
    <h2>Tutors Table</h2>
    <p>Table mapping to the Tutor model.</p>
    <table class="table table-bordered table-model">
        <thead class="thead-dark">
            <tr>
                <th colspan="2">Tutors</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td colspan="1" class="table-col">
                    ID
                </td>
                <td colspan="1"class="table-col">
                    (PK) Int
                </td>
            </tr>
            <tr>
                <td class="col-1">
                    FirstName
                </td>
                <td class="col-1">
                    String NN
                </td>
            </tr>
            <tr>
                <td class="col-1">
                    LastName
                </td>
                <td class="col-1">
                    String
                </td>
            </tr>
            <tr>
                <td class="col-1">
                    ProtopageURL
                </td>
                <td class="col-1">
                    String
                </td>
            </tr>
            <tr>
                <td class="col-1">
                    Email
                </td>
                <td class="col-1">
                    String
                </td>
            </tr>
            <tr>
                <td class="col-1">
                    Phone
                </td>
                <td class="col-1">
                    String
                </td>
            </tr>
            <tr>
                <td class="col-1">
                    MobilePhone
                </td>
                <td class="col-1">
                    String
                </td>
            </tr>
            <tr>
                <td class="col-1">
                    BusinessId
                </td>
                <td class="col-1">
                    Int
                </td>
            </tr>

        </tbody>
    </table>
</div>





